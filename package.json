  "description": "Modern React e-learning platform with enhanced animations and design",
  "homepage": "https://sainivaibhav742.github.io/devigram",
  "private": true,
  "author": "Devigram Team",
  "keywords": ["react", "e-learning", "education", "courses", "mentorship"],
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
